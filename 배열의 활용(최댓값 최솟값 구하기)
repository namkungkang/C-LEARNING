#include<stdio.h>
int main() {
 int n,max,min,i;
 int arr[1000];
 printf("입력할 개수 :"); 
 scanf("%d", &n);
  for(i=0; i<n; i++) {
 	scanf("%d",&arr[i]);
 }
 max=arr[0];
 for(i=1; i<n; i++) {
 	if(max<arr[i]) {
 	   max = arr[i];	
		  }
		  
 }
  printf("최댓값:%d\n", max);
  
  for(i=1; i<n; i++) {
 	if(min>arr[i]) {
 	   min = arr[i];	
		  }
		  
 }
  printf("최솟값: %d", min);
}
